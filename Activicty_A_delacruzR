#number1
print("number 1")
for x in range (1,17,3):
    print(x, end='\t')
print()
#number2
print("number 2")
for x in range (100,75,-7):
    print(x, end='\t')
print()
#number3
print("number 3")
for x in range(1,5):
    if x % 3 == 0:
        print("1", end ='\t')
    else:
        print("0", end ='\t')
